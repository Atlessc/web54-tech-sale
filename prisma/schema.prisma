// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("VITE_DATABASE_URL")
}

model Product {
  id          Int     @id @default(autoincrement())
  image       String
  name        String
  description String
  price       Float
  availability String
  quantity     Int
}

model AdminUser {
  id       Int      @id @default(autoincrement())
  email    String   @unique
  password String
  name     String
}

